{
	"extends": [
		"prettier",
		"plugin:prettier/recommended"
	],
	"plugins": [
		"prettier"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	},
	"rules": {
		"quotes": [1,"single", {"avoidEscape": true}],
		"prettier/prettier": "error"
	}
}